{
  "manifest_version": 3,
  "name": "Quick search",
  "description": "Navigate Chrome easily",
  "version": "0.0.2",
  "permissions": ["activeTab", "scripting", "browsingData", "topSites"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {},
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E"
      }
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
