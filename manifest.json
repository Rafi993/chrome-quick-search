{
  "manifest_version": 3,
  "name": "Quick search",
  "description": "Navigate Chrome easily",
  "version": "0.0.1",
  "permissions": ["tabs", "storage", "activeTab", "scripting", "browsingData"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {},
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+E",
        "mac": "Command+E"
      }
    }
  }
}
